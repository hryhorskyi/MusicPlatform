{
  "description": "The schema for sessions post method. Contain token format for response body.",
  "required": [
      "csrf",
      "access",
      "access_expires_at",
      "refresh",
      "refresh_expires_at"
  ],
  "properties": {
      "csrf": {
          "type": "string"
      },
      "access": {
          "type": "string"
      },
      "access_expires_at": {
          "type": "string",
          "format": "date-time"
      },
      "refresh": {
          "type": "string"
      },
      "refresh_expires_at": {
          "type": "string",
          "format": "date-time"
      }
  }
}
